<h1>Tickets</h1>

<table class="table table-hover">
	<thead>
		<tr>
			<th>
				#
			</th>
			<th>
				Title
			</th>
			<th>
			  Owner
			</th>
			<th>
				State
			</th>
			<th>
				Priority
			</th>
			<th>
				Due Date
			</th>
			<th>
				Date Completed
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<%= render @tickets.order(:due_date, :priority) %>
	</tbody>
</table>